<Window x:Class="MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NumberGenUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
	<Grid>
		<TextBox HorizontalAlignment="Left" Margin="280,199,0,0" Text="Number 1" TextWrapping="Wrap" VerticalAlignment="Top" Width="94" Height="18" TextChanged="TextBox_TextChanged" Name="Text1"/>
		<TextBox HorizontalAlignment="Left" Margin="280,222,0,0" Text="Number 2" TextWrapping="Wrap" VerticalAlignment="Top" Width="94" Height="18" TextChanged="TextBox_TextChanged" Name="Text2"/>
		<Button Content="Submit" HorizontalAlignment="Left" Margin="280,245,0,0" VerticalAlignment="Top" Height="32" Width="94" Click="Button_Click" Name="SubmitButton"/>
		<TextBlock HorizontalAlignment="Left" Margin="280,300,0,0" Text="Output" TextWrapping="Wrap" VerticalAlignment="Top" Height="34" Width="94" Name="Output"/>

	</Grid>
</Window>
